<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/ActiveOrdersFragment">

    <fragment
        android:id="@+id/ActiveOrdersFragment"
        android:name="com.hellomotem.roxietest.presentation.activeorders.ActiveOrdersFragment"
        android:label="Активные Заказы"
        tools:layout="@layout/fragment_active_orders">
        <action
            android:id="@+id/action_ActiveOrders_to_Details"
            app:destination="@id/OrderDetailsFragment">
        </action>
    </fragment>

    <fragment
        android:id="@+id/OrderDetailsFragment"
        android:name="com.hellomotem.roxietest.presentation.ordersdetail.OrderDetailFragment"
        android:label="Детали Заказа"
        tools:layout="@layout/fragment_order_detail">
        <argument
            android:name="orderArgs"
            app:argType="com.hellomotem.roxietest.presentation.entity.ActiveOrderUi" />
        <action
            android:id="@+id/action_Details_to_ActiveOrders"
            app:destination="@id/ActiveOrdersFragment" />
    </fragment>
</navigation>